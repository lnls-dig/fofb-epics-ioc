    field(DTYP,"asynUInt32Digital")
    field(DESC,"get FOFB CC module to read configuration")
    field(SCAN,"I/O Intr")
    field(INP,"@asynMask($(PORT),$(ADDR),0xFFFFFFFF,$(TIMEOUT))FOFB_CC_CFG_VAL_ACT_PART")
}

