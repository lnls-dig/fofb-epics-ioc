record(longout,"$(P)$(R)FofbCtrlActPart-SP"){
    field(DTYP,"asynUInt32Digital")
    field(DESC,"set FOFB CC module to read configuration")
    field(SCAN,"Passive")
    field(OUT,"@asynMask($(PORT),$(ADDR),0xFFFFFFFF,$(TIMEOUT))FOFB_CC_CFG_VAL_ACT_PART")
    field(PINI,"YES")
}
