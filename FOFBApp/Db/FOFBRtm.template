record(calcout, "$(S)$(RTM_CHAN)AlarmsAmp-Mon"){
    field(DESC, "Over-temperature and over-current flags")
    field(SCAN, ".1 second")
    field(INPA, "$(S)$(RTM_CHAN)PSAmpOverCurrFlagL CA")
    field(INPB, "$(S)$(RTM_CHAN)PSAmpOverTempFlagL CA")
