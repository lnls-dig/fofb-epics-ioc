    field(DTYP,"asynUInt32Digital")
    field(DESC,"get RTM LAMP status register")
    field(SCAN,"I/O Intr")
    field(INP,"@asynMask($(PORT),$(ADDR),0xFFFFFFFF,$(TIMEOUT))RTMLAMP_OHWR_STA")
}

