record(longout,"$(P)$(R)RtmLampStatus-SP"){
    field(DTYP,"asynUInt32Digital")
    field(DESC,"set RTM LAMP status register")
    field(PINI,"YES")
    field(SCAN,"Passive")
    field(OUT,"@asynMask($(PORT),$(ADDR),0xFFFFFFFF,$(TIMEOUT))RTMLAMP_OHWR_STA")
}
